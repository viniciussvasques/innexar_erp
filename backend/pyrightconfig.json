{
  "include": [
    "apps",
    "config",
    "manage.py"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/migrations",
    "**/venv",
    "**/.venv",
    "**/env"
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.11",
  "pythonPlatform": "All",
  "typeCheckingMode": "basic",
  "venvPath": ".",
  "venv": "venv",
  "stubPath": "",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.11",
      "pythonPlatform": "All",
      "extraPaths": []
    }
  ]
}

