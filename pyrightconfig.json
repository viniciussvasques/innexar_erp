{
  "include": [
    "backend"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/migrations",
    "**/venv",
    "**/.venv",
    "**/env",
    "frontend",
    "admin-panel"
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.11",
  "pythonPlatform": "All",
  "typeCheckingMode": "basic",
  "venvPath": ".",
  "venv": "venv",
  "stubPath": ""
}

